<ul>
<% @search.results.each do |person| %>
    <%  if person.attending? %>
        <li>Name; <%= person.first_name %> <%= person.last_name %><br />
        Position/Company: <%= person.role %> <%= person.company %><br />
        Keywords: <%= person.keywords %>
        **<%= person.attending %>**
    <% end %>
<% end %>
</ul>